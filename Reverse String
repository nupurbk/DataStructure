//reverse words in strings
	static void reverseString(String abc){
	    //System.out.print(abc); 
	    String[] words=abc.split("\\.");
	    
	    
	    StringBuilder sb=new StringBuilder();
	    for(int i=words.length-1;i>=0;i--){
	        sb.append(words[i]);
	        sb.append(".");
	    }
	    
	    System.out.println(sb.toString());
	    
	    
	}
	
